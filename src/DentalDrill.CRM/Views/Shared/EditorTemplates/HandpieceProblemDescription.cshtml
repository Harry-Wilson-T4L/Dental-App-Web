@model String

@(Html.Kendo().AutoCompleteFor(x => x)
      .DataTextField("Name")
      .ValuePrimitive(true)
      .Filter("startswith")
      .MinLength(2)
      .DataSource(dataSource => dataSource
          .Ajax()
          .Read(read => read.Action("ProblemsAutoComplete", "Jobs"))
          .Sort(sort =>
          {
              sort.Add("Name");
          })
          .ServerOperation(true)))